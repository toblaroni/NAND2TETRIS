/**
 * Exclusive-or gate:
 * out = (not(a) and b) or (a and not(b))
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand (a=a, b=b, out=aNandb);
    And (a=a, b=aNandb, out=aNandbAnda);
    And (a=aNandb, b=b, out=aNandbAndb);
    Or (a=aNandbAnda, b=aNandbAndb, out=out);
}
